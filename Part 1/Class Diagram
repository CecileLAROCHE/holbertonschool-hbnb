classDiagram
direction TB
    class ClassName {
	    +created_at DateTime
	    +updated_at DateTime
      +id UUID
	    +save() void
      created()
	    updated()
	    deleted()
    }
    class User {
	    +FirstName String
	    +LastName String
	    +email
	    -Password
      +Administrator Boomean

    }
    class Place {
	    Title String
	    description String
	    price float
	    latitude Double
	    longitude Double
    }
    class Review {
	    place String
	    user String
	    rating String
	    comment String

	    listed()
    }
    class Amenity {
      Name string
      description string
	    created()
	    updated()
	    deleted()
	    listed()
    }

	note for Amenity "Equipements"
	note for Review "gestion des avis"

    ClassName <-- User
    ClassName <-- Place
    ClassName <-- Review
    ClassName <-- Amenity
    Place --> Review : has
    User --> Place : Owns
    Place --> Amenity : includes
